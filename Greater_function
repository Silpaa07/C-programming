//Write a program in C to find the greatest between two numbers.
#include<stdio.h>
int greater(int a,int b)
{
    if(a>b)
    printf("a is greater than b");
    else 
    printf("b is greater than a");
}
int main()
{
int a=0,b=0;
printf("Enter the values of a and b\n");
scanf("%d %d",&a,&b);
greater(a,b);
}
